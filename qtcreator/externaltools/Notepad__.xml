<externaltool id="Notepad__">
    <description>This tool prints a line of useful text</description>
    <displayname>Notepad++</displayname>
    <category>Text</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>C:/Program Files (x86)/Notepad++/notepad++.exe</path>
        <arguments>%{CurrentDocument:FilePath} -n%{CurrentDocument:Row} -c%{CurrentDocument:Column}</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
