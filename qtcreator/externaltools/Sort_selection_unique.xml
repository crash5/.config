<externaltool id="Sort_selection_unique">
    <description>Sorts the selected text and remove duplicated lines</description>
    <displayname>Sort selection unique</displayname>
    <category>Text</category>
    <executable output="replaceselection" error="showinpane" modifiesdocument="no">
        <path>c:/Program Files/Git/usr/bin/sort.exe</path>
        <arguments>-u</arguments>
        <input>%{CurrentDocument:Selection}</input>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
