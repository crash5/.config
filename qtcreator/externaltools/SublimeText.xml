<externaltool id="SublimeText">
    <description>This tool prints a line of useful text</description>
    <displayname>SublimeText</displayname>
    <category>Text</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>C:/Program Files/Sublime Text 3/sublime_text.exe</path>
        <arguments>%{CurrentDocument:FilePath}:%{CurrentDocument:Row}:%{CurrentDocument:Column}</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
