<externaltool id="VSCode">
    <description>This tool prints a line of useful text</description>
    <displayname>VSCode</displayname>
    <category>Text</category>
    <executable output="showinpane" error="showinpane" modifiesdocument="yes">
        <path>code</path>
        <arguments>-g %{CurrentDocument:FilePath}:%{CurrentDocument:Row}:%{CurrentDocument:Column}</arguments>
        <workingdirectory>%{CurrentDocument:Path}</workingdirectory>
    </executable>
</externaltool>
